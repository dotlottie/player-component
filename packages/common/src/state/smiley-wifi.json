{
  "descriptor": {
    "id": "smiley_wifi",
    "initial": "smileyState"
  },
  "states": {
    "smileyState": {
      "animationId": "smiley",
      "statePlaybackSettings": {
        "autoplay": true,
        "loop": true,
        "direction": -1,
        "speed": 1,
        "defaultTheme": "bounce-dark"
      },
      "onMouseEnter": {
        "state": "wifiState"
      }
    },
    "wifiState": {
      "animationId": "wifi",
      "statePlaybackSettings": {
        "autoplay": true,
        "loop": false,
        "speed": 3,
        "direction": 1,
        "defaultTheme": "wifi-dark"
      },
      "onComplete": {
        "state": "smileyState"
      }
    }
  }
}
