{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "globalDependencies": ["tsconfig.build.json", "tsconfig.dev.json", "tsconfig.json"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "cache": false,
      "dependsOn": ["build"],
      "outputs": []
    },
    "test:coverage": {
      "cache": false,
      "dependsOn": ["build"],
      "outputs": ["coverage/**"]
    },
    "type-check": {
      "cache": false
    },
    "lint": {
      "cache": false,
      "outputs": []
    },
    "update-releases": {
      "dependsOn": ["^update-releases"]
    },
    "clean": {},
    "dev": {
      "cache": false
    }
  }
}
